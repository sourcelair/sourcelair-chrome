{
    "manifest_version": 2,
    "name": "SourceLair",
    "version": "1.1.1",
    "default_locale": "en",
    "description": "In-browser IDE",
    "icons": {
        "128": "icon_128.png"
    },
    "app": {
        "urls": ["https://www.sourcelair.com/"],
        "launch": {
            "web_url": "https://www.sourcelair.com"
        }
    },
    "author": "SourceLair, Limited",
    "permissions": [
        "unlimited_storage",
        "notifications"
    ]
}